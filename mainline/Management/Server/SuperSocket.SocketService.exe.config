<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="socketServer" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine" />
  </configSections>
  <appSettings>
    <add key="ServiceName" value="SupperSocketService" />
  </appSettings>
  <connectionStrings/>
  <socketServer>
    <servers>
      <server name="SuperWebSocket"
              serviceName="SuperWebSocket"
              ip="Any" port="2011"
              maxConnectionNumber="100">
      </server>
      <server name="Management"
              serviceName="Management"
              ip="Any" port="2012"
              managePassword="supersocket"
              manageUser="admin"
              maxConnectionNumber="5">
      </server>
    </servers>
    <services>
      <service name="SuperWebSocket" type="SuperWebSocket.WebSocketServer, SuperWebSocket" />
      <service name="Management" type="SuperSocket.Management.Server.ManagementServer, SuperSocket.Management.Server" />
    </services>
  </socketServer>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
</configuration>
