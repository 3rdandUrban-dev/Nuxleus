<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <section name="socketServer" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine"/>
    </configSections>
    <appSettings>
        <add key="ServiceName" value="SocksService"/>
    </appSettings>
    <socketServer>
        <servers>
            <server name="SocksServer"
                    serviceName="socksService"
                    ip="Any" port="911" mode="Async">
            </server>
        </servers>
        <services>
            <service name="socksService"
		             type="SuperSocket.QuickStart.SocksServer.SocksServer, SuperSocket.QuickStart.SocksServer" />
        </services>
    </socketServer>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
    </startup>
</configuration>