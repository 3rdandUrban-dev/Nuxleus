<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />
		<section name="log4net" type="System.Configuration.IgnoreSectionHandler" />
	</configSections>
	<unity>
		<aliases>
			<add alias="DocumentPanel" type="Lextm.SharpSnmpLib.Compiler.DocumentPanel, Compiler" />
			<add alias="DocumentListPanel" type="Lextm.SharpSnmpLib.Compiler.DocumentListPanel, Compiler" />
			<add alias="ModuleListPanel" type="Lextm.SharpSnmpLib.Compiler.ModuleListPanel, Compiler" />
			<add alias="DockContent" type="WeifenLuo.WinFormsUI.Docking.DockContent, WeifenLuo.WinFormsUI.Docking" />
			<add alias="OutputPanel" type="Lextm.SharpSnmpLib.Compiler.OutputPanel, Compiler" />
			<add alias="Assembler" type="Lextm.SharpSnmpLib.Mib.Assembler, SharpSnmpLib.Mib" />
			<add alias="Parser" type="Lextm.SharpSnmpLib.Mib.Parser, SharpSnmpLib.Mib" />
			<add alias="Compiler" type="Lextm.SharpSnmpLib.Compiler.CompilerCore, Compiler" />
			<add alias="MainForm" type="Lextm.SharpSnmpLib.Compiler.MainForm, Compiler" />
			<add alias="singleton" type="Microsoft.Practices.Unity.ContainerControlledLifetimeManager, Microsoft.Practices.Unity" />
		</aliases>
		<containers>
			<container name="compiler">
				<register type="Compiler">
					<lifetime type="singleton" />
					<property name="Parser" propertyType="Parser">
						<dependency />
					</property>
					<property name="Assembler" propertyType="Assembler">
						<dependency />
					</property>
				</register>
				<register type="Parser" />
				<register type="Assembler">
					<lifetime type="singleton" />
					<constructor>
						<param name="folder" parameterType="System.String">
							<value value="modules" />
						</param>
					</constructor>
				</register>
				<register type="DockContent" mapTo="OutputPanel" name="Output">
					<lifetime type="singleton" />
				</register>
				<register type="DockContent" mapTo="DocumentListPanel" name="DocumentList">
					<lifetime type="singleton" />
					<property name="Compiler" propertyType="Compiler">
						<dependency />
					</property>
				</register>
				<register type="DockContent" mapTo="ModuleListPanel" name="ModuleList">
					<lifetime type="singleton" />
					<property name="Assembler" propertyType="Assembler">
						<dependency />
					</property>
					<property name="Compiler" propertyType="Compiler">
						<dependency />
					</property>
				</register>
				<register type="MainForm" name="">
					<lifetime type="singleton" />
					<property name="Compiler" propertyType="Compiler">
						<dependency />
					</property>
				</register>
			</container>
		</containers>
	</unity>
	<log4net>
		<!-- Define some output appenders -->
		<appender name="OutputPanelAppender" type="Lextm.SharpSnmpLib.OutputPanelAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%message%newline" />
			</layout>
		</appender>
		<!-- Setup the root category, add the appenders and set the default level -->
		<root>
			<level value="WARN" />
		</root>
		<!-- Specify the level for some specific categories -->
		<logger name="Lextm.SharpSnmpLib.Compiler">
			<level value="ALL" />
			<appender-ref ref="OutputPanelAppender" />
		</logger>
		<logger name="Lextm.SharpSnmpLib.Mib">
			<level value="ALL" />
			<appender-ref ref="OutputPanelAppender" />
		</logger>
	</log4net>
	<startup>
		<supportedRuntime version="v2.0.50727" />
	</startup>
</configuration>