        <Key word="PIB-DEFINITIONS" />
        <Key word="PLUSINFINITY" />
        <Key word="PRESENT" />
        <Key word="PrintableString" />
        <Key word="PRIVATE" />
        <Key word="REAL" />
        <Key word="RELATIVE" />
        <Key word="RESULT" />
        <Key word="SEQUENCE" />
        <Key word="SET" />
        <Key word="SIZE" />
        <Key word="STATUS" />
        <Key word="STRING" />
        <Key word="SYNTAX" />
        <Key word="TAGS" />
        <Key word="TeletexString" />
        <Key word="T61String" />
        <Key word="TRUE" />
        <Key word="TYPE-IDENTIFIER" />
        <Key word="UNION" />
        <Key word="UNIQUE" />
        <Key word="UNIVERSAL" />
        <Key word="UniversalString" />
        <Key word="UTCTime" />
        <Key word="UTF8String" />
