class IronPythonCommunityEdition(AutoPackageRecipe):
    name = 'IronPython-MS-Dev'
    version = '1.1a1'

    buildRequires = [ 
        'PyXML:python', 
        'mono:lib',
		'mono:runtime',
        'sgmlop:python',
        'pycrypto:python'
       ]

    def setup(r):
        r.addArchive('http://s3.amazonaws.com/IronPython/IronPython-%(version)s-Bin.zip')
        r.addSource('makefile')
