<service:operation xmlns:at="http://atomictalk.org" xmlns:metadata="http://xameleon.org/service/metadata" xmlns:header="http://xameleon.org/service/http/header" xmlns:service="http://xameleon.org/service" xmlns:operation="http://xameleon.org/service/operation" xmlns:proxy="http://xameleon.org/service/proxy" xmlns:param="http://xameleon.org/service/session/param">
  <param:uri>{query-string:uri}</param:uri>
  <operation:json-to-xml>
    <proxy:get-json uri="{$uri}">
