    <page:service>
      <service:ip-location href="@@service-gateway@@/ipgeolocator/geocode" />
      <service:language-selector href="@@TODO@@" />
    </page:service>
    <page:model>
      <model:lang binding="@@atom@@">
        @@TODO|$comment: Data that binds a language code with the proper URI to gain access to content in the specified language$|@@ 
      </model:lang>
      <model:menu binding="@@atom@@">
        @@TODO|$comment: $|@@
      </model:menu>
    </page:model>
    <page:view>
      <view:search-form xmlns:form="http://atomictalk.org/page/view/form">
        <form:search-form type="html" id="@@id.generate|default:@id@|@@" method="@@search-form.method@@" action="@@search-form.action@@" target="@@target.default@@" class="@@class.default@@">
          <form:input type="text" name="@name@|required|" id="@@id.generate|default:@id@|@@" maxlength="@@search-form.maxlength@@" value="@@search-form.text@@" onclick="if (this.value == '@@search-form.text@@') this.value = ''; return true;" onblur="if (this.value == '') this.value = '@@search-form.text@@'; return true;" class="@@class.default@@" />
          <form:input type="hidden" id="@@id.generate|default:@id@|@@" name="@name@|required|" value="@value@|required|" class="@@class.default@@" />
          <form:input type="submit" id="@@id.generate|default:@id@|@@" value="@@search-form.input@@" class="@@class.default@@" />
        </form:search-form>
      </view:search-form>
      <view:menu>
        <menu:list type="html" id="@@id.generate|default:@id@|@@">
          <menu:item type="label">@label@</menu:item>
          <menu:item type="link">
            <html:a href="@href@">@label@</html:a>
          </menu:item>
        </menu:list>
      </view:menu>
    </page:view>
    <page:resource>
      <resource:ip-location type="string" src="@@service-gateway@@@@service:ip-location@@" />
      <resource:help-support type="page" href="@@base-uri@@/support/" label="Help/Support" />
      <resource:create-new-account type="page" href="@@base-uri@@/create-new-account/" label="Create New" />
      <resource:login type="page" href="@@base-uri@@/login/" label="Login" />
      <resource:lang type="string" src="@src@" label="model:lang/$@@language@@/@label" />
    </page:resource>
