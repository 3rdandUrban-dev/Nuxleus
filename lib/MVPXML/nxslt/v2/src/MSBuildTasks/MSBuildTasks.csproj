ilasm nxslt.fixed.il /RESOURCE=nxslt.res /DLL /OUTPUT="$(TargetFileName)" /KEY="$(ProjectDir)../nxslt.snk"
@echo ########### Cleaning...
del nxslt.res
del *.il
del *.resources
@echo ########### nxslt Custom Build Event has finished
</PostBuildEvent>
