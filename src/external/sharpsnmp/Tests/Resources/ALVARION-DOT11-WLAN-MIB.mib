--
-- Private Traps for BreezeAccessVL
--

brzaccVLSUassociatedAUTRAP TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
    brzaccVLTrapSUMacAddr   -- SU MAC address
    }
  --&amp;ACTIONS { log, minor, clrdups }
  --&amp;MESG "New SU associated (SU Mac Address: $1)"
  DESCRIPTION
   "An AU trap indicating a new association with an SU."
  ::= 2

brzaccVLAUdisassociatedTRAP TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
    brzaccVLTrapSUMacAddr   --Unit MAC address
    }
  DESCRIPTION
   "An AU trap indicating that an SU has been disassociated
    from the AU.
    The AU decides that an SU has been disassociated from it and remove it
    from the ADB after receiving from another AU a SNAP frame with the SU MAC address.
    The SNAP frame indicating a network topology change where the SU has associated with
    another AU will be received if both AUs are connected to the same Ethernet backbone"
  ::= 3

brzaccVLAUagingTRAP TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
    brzaccVLTrapSUMacAddr  -- Unit MAC address
    }
  --&amp;ACTIONS { log, minor, clrdups }
  --&amp;MESG "Unit was aged out (SU Mac Address: $1)"
  DESCRIPTION
   "An AU trap indicating that the unit association was aged out
    and removed from the Associates SUs database following its failure
    to acknowledge a specified number of consecutive frames."
  ::= 4

brzaccVLSUassociatedTRAP TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
--   brzaccVLLastAUMacAddress,     Old AU mac address
    brzaccVLAssociatedAU       -- New AU mac address
--   brzaccVLTrapLastSNRQuality,     Average SNR with old AU
  }
  --&amp;ACTIONS { log, minor, clrdups }
  --&amp;MESG "Associated with AU ( Mac Address $2)"
  DESCRIPTION
   "An SU trap indicating association with an AU.
    In addition to the MAC address information of the AU, the
    trap also includes information on the average SNR of frames
    received from the AU."
  ::= 6


brzaccVLAUwirelessQualityTRAP TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
    brzaccVLTrapToggle,
    brzaccVLTrapRTx
    }
  --&amp;ACTIONS { log, major, clrdups }
  --&amp;MESG "Quality of wireless link has been changed to $3"
  DESCRIPTION
    "An AU trap, indicating that the quality of the wireless
    link (retransmissions rate) has changed and dropped below (Off) or increased
    above (On) a threshold , defined by the *brzaccVLWirelessTrapThreshold.
    The threshold for AU is a percentage of retransmissions."
  ::= 20

brzaccVLPowerUpFromReset TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
    brzaccVLUnitMacAddress  -- Unit MAC address
    }
  --&amp;ACTIONS { log, minor, clrdups }
  --&amp;MESG "Power Up From Reset (Unit Mac Address: $'1)"
  DESCRIPTION
   "An AU trap indicating power up after Reset."
  ::= 101

brzaccVLTelnetStatusTRAP TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
    brzaccVLTrapLog,
    brzaccVLTrapAccessRights,
    brzaccVLTrapTelnetUserIpAddress
    }
  --&amp;ACTIONS { log, major, clrdups }
  --&amp;MESG "$'1 With Access Rights $'3 (Telnet User Ip Address is $2)"
  DESCRIPTION
   "A trap indicating that a log-in or log-out has been performed
    via Telnet. Includes the log-in access
    right and the IP address of the PC performing Telnet."
  ::= 102

brzaccVLParameterChangedTRAP TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
    brzaccVLTrapParameterChanged
    }
  --&amp;ACTIONS { log, major, clrdups }
  --&amp;MESG "A $'1 Parameter Changed"
  DESCRIPTION
   "An AU or SU trap indicating a change in a IP Filter or VLAN parameter."
  ::= 103


brzaccVLFtpLoadingStatusTRAP TRAP-TYPE
  ENTERPRISE brzAccessVLProducts
  VARIABLES {
    brzaccVLTrapFtpStatus,
    brzaccVLUnitMacAddress
    }
  --&amp;ACTIONS { log, major, clrdups }
  --&amp;MESG "File Transfer to unit is finished"
  DESCRIPTION
    "A trap indicating that last FTP loading process has been
    finished successfully or failed. "
  ::= 104

